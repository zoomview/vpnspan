# VPNSpan Backend Environment Variables
# Copy this file to .env and fill in the values

# Server
PORT=5000
NODE_ENV=production

# Surfshark VPN (manual setup credentials)
SURFSHARK_USER=
SURFSHARK_PASS=
SURFSHARK_OVPN_PATH=/etc/openvpn/surfshark/us-nyc.prod.surfshark.com_tcp.ovpn

# ProtonVPN
PROTONVPN_USER=
PROTONVPN_PASS=
PROTONVPN_OVPN_PATH=/etc/openvpn/protonvpn/us-free-110.protonvpn.tcp.ovpn
